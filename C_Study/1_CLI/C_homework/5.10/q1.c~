#include <stdio.h>

void main(){
	int number1=0;
	int number2=0;
	int number3=0;
	int number4=0;


	while(number3 < 5){
		printf("정수를 입력하세요: ");
		scanf("%d", &number2);

		while(number2 < 1){
			printf("1 이상의 정수를 입력하세요: ");
			scanf("%d", &number2);
		}
		number4=number2+number4;
		number3++;
	}
	printf("정수의 합은 %d 입니다.\n", number4);
}
