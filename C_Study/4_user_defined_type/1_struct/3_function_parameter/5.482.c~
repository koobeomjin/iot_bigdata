#include <stdio.h>
#pragma warning (disable : 4996)

typedef struct{
	int xpos;
	int ypos;
} Point;

typedef struct{
	char name[20];
	int xpos;
	int ypos;
} Point2;

void main(){
	Point pos1 = { 1,2 };
	Point pos2;
	Point2 pos3;
	int arr[2] = { 1,2 };

	pos2 = pos1;
	// 구조체의 대입 연산은 같은 형에 대해서만 가능하다.
	// pos3 = pos1;
	pos2 = pos2 + pos1;
}
